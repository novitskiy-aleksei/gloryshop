	// Call template init (optional, but faster if called manually)
	$(document).ready(function() { 
		$('#icons a').live('click dblclick', function() {
			var fa_class = $(this).children('i').attr('class');
			parent.$('#<?php echo $thumb; ?>').attr('class',fa_class);
			parent.$('#<?php echo $field; ?>').attr('value',fa_class );
			parent.$('.modal-box').modal('hide');		
			parent.$('.modal-backdrop').remove();	
		});
		$('#icons a').live('dblclick', function() {
				parent.$('#modal-box .modal-header .close').trigger('click');
		});
		$('#tab_popup_icons a').click( function(){
			localStorage.setItem("tab_popup_icons", $(this).attr("href") );
		} );

		if( localStorage.getItem("tab_popup_icons") !="undefined" ){
			$('#tab_popup_icons a').each( function(){ 
				if( $(this).attr("href") ==  localStorage.getItem("tab_popup_icons") ){
					$(this).click();
					return ;
				}
			} );
			
		}
		
	});	
	</script>        
